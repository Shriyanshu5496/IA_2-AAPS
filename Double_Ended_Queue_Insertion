#include <iostream>
#include <deque>
using namespace std;

int main() {
    deque<int> dq;

    // Insert at front
    dq.push_front(10);
    dq.push_front(20);

    // Insert at rear
    dq.push_back(30);
    dq.push_back(40);

    // Display deque
    for (int x : dq) cout << x << " ";
    cout << "\n";

    return 0;
}
